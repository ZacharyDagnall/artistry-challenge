<h1> Artist information! </h1>

<h3> <%= @artist.name %> </h3> 
<h3> <%= @artist.age %> </h3>
<h3> <%= @artist.title %> </h3>

<ul>
    <% @artist.instruments.each do |instrument_inst| %>
        <li> <%= instrument_inst.name %> </li>
    <% end %>
</ul>

<%= button_to "Delete artist", artist_path(@artist), method: "delete" %>
<%= link_to "Edit artist", edit_artist_path(@artist) %>